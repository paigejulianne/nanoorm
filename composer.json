{
    "name": "paigejulianne/nanoorm",
    "description": "A lightweight, full-featured PHP ORM with fluent query builder, relationships, and migrations",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "orm",
        "database",
        "pdo",
        "active-record",
        "query-builder",
        "mysql",
        "postgresql",
        "sqlite",
        "migrations"
    ],
    "authors": [
        {
            "name": "Paige Julianne",
            "email": "paige@example.com"
        }
    ],
    "require": {
        "php": ">=8.1",
        "ext-pdo": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0 || ^11.0"
    },
    "autoload": {
        "psr-4": {
            "NanoORM\\": ""
        },
        "files": [
            "NanoORM.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "NanoORM\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
